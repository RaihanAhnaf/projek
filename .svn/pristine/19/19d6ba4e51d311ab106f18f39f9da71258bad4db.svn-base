}
.k-widget.k-multiselect{
    vertical-align: middle;
    display: inline-block;
}
.page-template {
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
}

.page-template .header {
    position: absolute;
    top: 30px;
    left: 30px;
    right: 30px;

    border-bottom: 1px solid #888;

    text-align: center;
    font-size: 18px;
}

.page-template .footer {
    position: absolute;
    bottom: 30px;
    left: 30px;
    right: 30px;
}