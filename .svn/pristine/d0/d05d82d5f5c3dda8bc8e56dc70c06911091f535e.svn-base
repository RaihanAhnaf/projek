<script type="text/javascript">
    var userinfo = {
        usernameh: ko.observable('{{.Username}}'),
        menunameh: ko.observable('{{.Menuname}}'),
    }
</script>

<script src="{{BaseUrl}}res/main/js/master-invoicecategory.js" type="text/javascript"></script>
<link rel="stylesheet" href="{{BaseUrl}}res/main/css/master.css">
<div class="page-bar">
    <ul class="page-breadcrumb">
        <li>
            <span class="bar">Invoice Category</span>
            <i class="fa fa-circle"></i>
        </li>
        <li>
            <a href="/master/invcategory">Invoice Category</a>
        </li>
    </ul>
    <div class="page-toolbar">
        <div class="pull-right tooltips btn" data-container="body" data-placement="bottom" data-original-title="date now">
            <i class="fa fa-calendar" aria-hidden="true"></i>&nbsp;
            <span class="thin uppercase hidden-xs font2" data-bind="text:invcategory.DatePageBar"></span>&nbsp;
        </div>
    </div>
</div>
<div class="row clearfix">
    <div id="usersett">
        <div class="col-md-12  panel-content">
            <div class="portlet box twis">
                <div class="portlet-title title-padding" >
                    <div class="caption ">
                        Invoice Category
                    </div>
                    <div class="tools tool-padding" >                        
                        <button onclick="invcategory.exportExcel()" type="button" class="btn btn-sm btn-outline green"><i class="fa fa-file-excel-o"></i>&nbsp;Export
                            To Excel</button>
                    </div>
                </div>
            
                    <div class="portlet-body">
                        <div class="col-md-16" data-bind="visible: !model.Processing()">
                            <div class="row">
                                <div class="col-md-12 bottom-margin">
                                    <div class="panel-heading">
                                        <div class="row">
                                            <div class="col-md-12 panel-padding">
                                                <div class="tab-content">
                                                    <div role="tabpanel" class="tab-pane active" id="List">
                                                        <div class="tab-pane-content">
                                                            <div class="row">
                                                                <div class="col-md-12" data-bind="visible: !model.Processing()" >
                                                                    <div class="grid-category padding-grid" id="gridinvcategory"></div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            {{template "_loader2.html"}}
                        </div>
                    </div>
                </div>
            </div>
            <!--form modal-->
            <div class="modal fade" id="AddNewModal" aria-hidden="true" aria-labelledby="modalTitle" role="dialog" tabindex="-1">
                <div class="modal-dialog" data-bind="visible: !model.Processing()">
                    <div class="modal-content konten-category margin16">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">Ã—</span>
            </button>
                            <h4 class="modal-title">Category</h4>
                        </div>
                        <div class="modal-body">
                            <div class="form-input row" data-bind="visible: !model.Processing()">
                                <div class="form-group col-md-11" style="visibility: hidden;">
                                    <label class="control-labeltrx col-md-4 onlabel">Category Code</label>
                                    <div class="control-input col-md-6 pads">
                                         <input type="text" class="form-control input-inline input-medium" name="code" id="code" placeholder="Type Code">
                                        <span class="k-invalid-msg" data-for="code"></span>
                                    </div>
                                </div>
                                <div class="form-group col-md-11">
                                    <label class="control-labeltrx col-md-4 onlabel">Category Name</label>
                                    <div class="control-input col-md-6 pads">
                                         <input type="text" class="form-control input-inline input-medium text-caps" name="name" id="name" placeholder="Type Name">
                                        <span class="k-invalid-msg" data-for="name"></span>
                                    </div>
                                </div>

                                <div class="form-group col-md-11 align-right">
                                    <button type="button" class="btn btn-sm btn-success btn-flat" onclick="invcategory.SaveData()"><i class="fa fa-floppy-o"></i>&nbsp;<span data-bind="text:invcategory.text"></span></button>
                                </div>
                            </div>
                            {{template "_loader2.html"}}
                        </div>
                    </div>
                </div>
            </div>
</div>
</div>